PROGRAM main
  VAR
    R_TRIG0 : R_TRIG;
    Pulse_Counter : BOOL;
    CTU0 : CTU;
    R_TRIG1 : R_TRIG;
    Pulse_Reset : BOOL;
  END_VAR
  VAR
    Relay_Start AT %QX4.0 : BOOL;
    Start_State AT %QX4.1 : BOOL;
    Relay_Downtime AT %QX4.2 : BOOL;
    Downtime_State AT %QX4.3 : BOOL;
    Counter_Trigger AT %QX4.4 : BOOL;
    Reset_Counter AT %QX4.5 : BOOL;
    Counter_Register AT %MW10 : INT;
  END_VAR

  Start_State := Relay_Start;
  Downtime_State := Relay_Downtime;
  R_TRIG0(CLK := Counter_Trigger);
  Pulse_Counter := R_TRIG0.Q;
  CTU0(CU := Pulse_Counter, R := Pulse_Reset, PV := 999);
  Counter_Register := CTU0.CV;
  IF Pulse_Counter THEN
    Counter_Trigger := FALSE; (*reset*)
  END_IF;
  R_TRIG1(CLK := Reset_Counter);
  Pulse_Reset := R_TRIG1.Q;
  IF Pulse_Reset THEN
    Reset_Counter := FALSE; (*reset*)
  END_IF;
END_PROGRAM


CONFIGURATION Config0

  RESOURCE Res0 ON PLC
    TASK task0(INTERVAL := T#20ms,PRIORITY := 1);
    PROGRAM instance0 WITH task0 : main;
  END_RESOURCE
END_CONFIGURATION

